{
	"name": "davejtoews/silverstripe-signaturefield",
	"description": "A signaturefield based on https://github.com/szimek/signature_pad",
	"type": "silverstripe-vendormodule",
	"keywords": ["silverstripe", "forms", "signature"],
    "license": "MIT",
	"homepage": "http://github.com/davejtoews/silverstripe-signaturefield",
	"authors": [
	{
		"name": "David Toews",
		"email": "davejtoews@gmail.com"
	}
	],
	"require":
	{
		"silverstripe/framework": "^4.0",
		"silverstripe/admin": "^1.1"
	},
	"extra": {
        "expose": [
            "css",
            "javascript"
        ]
    },
    "autoload": {
        "psr-4": {
            "DaveJToews\\SignatureField\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
